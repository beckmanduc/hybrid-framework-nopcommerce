			<class name="com.nopcommerce.user.Level_05_Page_Factory" />
		</classes>
	</test> 
	
	<test name="Run on EDGE">
	<parameter name="browser" value="edge" />
		<classes>
			<class name="com.nopcommerce.user.Level_05_Page_Factory" />
		</classes>
	</test> 
	
</suite>
